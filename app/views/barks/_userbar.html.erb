<div>
  <%= image_tag("shared/logofeed.png", class: "feed-logo") %>
</div>
<div>
  <%= current_user.photo.attached? ? cl_image_tag(current_user.photo.key, class: "avatar") : image_tag("users/blankuser.png", class: "avatar") %>
</div>
<div class="user-info">
  <h3><%= current_user.name %></h3>
  <p>@<%= current_user.handle %></p>
  <p><%= current_user.created_at.strftime("Member Since: %m/%d/%Y") %></p>
  <%= link_to "Log out", destroy_user_session_path, data: {turbo_method: :delete}, class: "barker-button" %>
</div>
